<?xml version="1.0"?>
<bindings id="wf2-bindings"
          xmlns="http://www.mozilla.org/xbl"
          xmlns:html="http://www.w3.org/1999/xhtml"
          xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
          xmlns:xbl="http://www.mozilla.org/xbl">

<binding id="wf2-form">
 <implementation>
  <property name="accept"/>
  <property name="data"/>
  <property name="elements" readonly="true"/>
  <property name="length" readonly="true"/>
  <property name="replace"/>
  <property name="templateElements" readonly="true"/>
  <method name="checkValidity"/>
  <method name="dispatchFormChange"/>
  <method name="dispatchFormInput"/>
  <method name="resetFromData"/>
  <method name="submit"/>
 </implementation>
</binding>

<binding id="wf2-output">
 <implementation>
  <property name="defaultValue"/>
  <property name="form"/>
  <property name="forms" readonly="true"/>
  <property name="labels" readonly="true"/>
  <property name="validationMessage" readonly="true"/>
  <property name="validity" readonly="true"/>
  <property name="value"/>
  <property name="willValidate" readonly="true"/>
  <method name="checkValidity"/>
  <method name="setCustomValidity"/>
 </implementation>
</binding>

<binding id="wf2-input">
 <implementation>
  <property name="autofocus" readonly="true"/>
  <property name="disabled"/>
  <property name="form" readonly="true"/>
  <property name="forms" readonly="true"/>
  <property name="labels" readonly="true"/>
  <property name="htmlTemplate" readonly="true"/>
  <property name="list"/>
  <property name="max"/>
  <property name="min"/>
  <property name="pattern"/>
  <property name="readOnly"/>
  <property name="required"/>
  <property name="selectedOption" readonly="true"/>
  <property name="step"/>
  <property name="type" readonly="true"/>
  <property name="validationMessage" readonly="true"/>
  <property name="validity" readonly="true"/>
  <property name="value"/>
  <property name="valueAsDate"/>
  <property name="valueAsNumber"/>
  <property name="willValidate" readonly="true"/>
  <method name="checkValidity"/>
  <method name="dispatchChange"/>
  <method name="dispatchFormChange"/>
  <method name="setCustomValidity"/>
  <method name="stepDown"/>
  <method name="stepUp"/>
 </implementation>
</binding>

